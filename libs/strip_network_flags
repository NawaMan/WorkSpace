#!/usr/bin/env bash
# Read tokens on argv, drop any --network/--net flag (and its value),
# and print the remaining tokens one-per-line.
set -euo pipefail

skip_next=false
for arg in "$@"; do
  if $skip_next; then
    skip_next=false
    continue
  fi
  case "$arg" in
    --network|--net)
      skip_next=true ;;              # drop this and the *next* token
    --network=*|--net=*)
      ;;                             # drop this single token
    *)
      printf '%s\n' "$arg" ;;
  esac
done
